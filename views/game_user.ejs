<!-- include header -->
<%- include('include/_header') %>
<!-- /include header -->
    <!-- Header -->
    <header id="header">
        <nav>
            <div class="container">
                <div class="text-center">
                    <a href="/" class="nav-brand text-dark">User Management System</a>
                </div>
            </div>
        </nav>
    </header>
<!-- /Header -->

<main id="site-main">
    <div class="container">
<div class="box-nav d-flex justify-between">
    <div class="filter">
        <a href="/admin"><i class="fas fa-angle-double-left"></i> All game</a>
    </div>
    <a href="/history-user" class="border-shadow">
        <span class="text-gradient">User History <class class="fas fa-history"></class></i></span>
    </a>
</div>
<form action="/" method="POST">
    <table class="table">
        <thead class="thead-dark">
            <tr>
                <th>ID</th>
                <th>Name</th>
                <th>Last Play</th>
                <th>Last Rank</th>
                <th>Duration</th>
            </tr>
            <tr>
                <% for(var i = 0; i < users.length; i++) { %>
                <td><%= i + 1 %></td>
                <td><%= game[i].name %></td>
                <td><%= game[i].lastPlay %></td>
                <td><%= game[i].lastRank %></td>
                <td><%= game[i].duration %></td>
            </tr>
            
        </thead>
        <tbody>

        </tbody>
    </table>
</form>
</div>
</main>



<!-- include footer -->
<%- include('include/_footer') %>
<!-- /include footer -->
